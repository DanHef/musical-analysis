{
  "name": "server",
  "description": "Server for Musical-Analysis",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Daniel0815",
    "email": "daniel-heffter@gmx.de"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run compile && npm run mocha",
    "test:debug": "npm run compile && npm run mocha:debug",
    "test:coverage": "nyc npm run mocha",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "mocha": "NODE_ENV=test ts-mocha \"test/**/*.test.ts\" -p test/tsconfig.json --recursive --exit",
    "mocha:debug": "NODE_ENV=test ts-mocha -r -p test/tsconfig.json test/**/*.test.ts --inspect --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^5.0.4",
    "feathers-permissions": "^2.0.3",
    "feathers-sequelize": "^6.2.0",
    "helmet": "^4.2.0",
    "mysql2": "^2.2.5",
    "sequelize": "^5.22.3",
    "serve-favicon": "^2.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bluebird": "^3.5.33",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/expect": "^24.3.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.0",
    "@types/serve-favicon": "^2.5.2",
    "@types/validator": "^10.11.3",
    "axios": "^0.21.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "shx": "^0.3.3",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.1",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
